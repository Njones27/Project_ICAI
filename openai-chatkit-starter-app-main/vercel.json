{
  "version": 2,
  "name": "agentkit-amazing",
  "builds": [
    {
      "src": "managed-chatkit/frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "managed-chatkit/frontend/dist"
      }
    },
    {
      "src": "managed-chatkit/backend/app/main.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "managed-chatkit/backend/app/main.py"
    },
    {
      "src": "/health",
      "dest": "managed-chatkit/backend/app/main.py"
    },
    {
      "src": "/(.*)",
      "dest": "managed-chatkit/frontend/dist/$1"
    }
  ],
  "env": {
    "OPENAI_API_KEY": "@openai-api-key",
    "CHATKIT_WORKFLOW_ID": "@chatkit-workflow-id",
    "ENVIRONMENT": "production"
  }
}
